#include <iostream>

int main()
{
    std::cout << " Enter two numbers: " << std::endl;
    int v1 = 0, v2 = 0;
    std::cin >> v1 >> v2;
    std::cout << " Number " << v1 << std::endl;
    while (v1 < v2)
    {
        ++v1;
        std::cout << " Number " << v1 << std::endl;
    }
    while (v1 > v2)
    {
        --v1;
        std::cout << " Number " << v1 << std::endl;
    }
    return 0;
}
//缺陷就是没有遵循从小到大的顺序，只能以 v1 作为打印的首个数字
